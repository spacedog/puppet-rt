use utf8;
<%- if @value.is_a? Hash-%>
Set(%<%= @title %>,
<% @value.sort.each do |k,v| -%>
  '<%= k %>' => '<%= v %>',
<%- end -%>
);
<%- elsif @value.is_a? Array -%>
Set(@<%= @title %>,
<% @value.sort.each do |v| -%>
  '<%= v %>',
<% end -%>
);
<%- else -%>
Set($<%= @title %>, '<%= @value %>');
<%- end -%>
1;
